

<%= form_for @cohort do |f| %>  
    <%= f.label :name %>  
    <%= f.text_field :name, value: @cohort.name%>  
    <%= f.label :start_date %>  
    <%= f.text_field :start_date, value: @cohort.start_date%>
    <%= f.label :end_date %>  
    <%= f.text_field :end_date, value: @cohort.end_date%> 
    <%= f.label :instructor %>  
    <%= f.select :instructor, [['Carissa Moore','Carissa Moore'],
                        ['Kelia Moniz','Kelia Moniz'],
                        ['Malia Manuel','Malia Manuel'],
                        ['Gabriela Reese','Gabriela Reese']],
                        {:multiple => true} %>
    <%= f.label :course %>  
    <%= f.select :course, [['Surfing 101','Surfing 101'],
                          ['Surfing 202','Surfing 202'],
                        ['Kite Surfing 101','Kite Surfing 101'],
                        ['Kite Surfing 202','Kite Surfing 202'],
                        ['Wake Surfing 101','Wake Surfing 101'],
                        ['Wakee Surfing 202','Wake Surfing 202'],
                        ['Board Maintenance','Board Maintenance']],
                        {:multiple => true} %>
    <%= f.label :student %>  
    <%= f.select :students, [['Maya Gabiera ','Maya Gabiera'],
                        ['Alana Blanchard','Alana Blanchard'],
                        ['Coco Ho','Coco Ho'],
                        ['Sage Erickson','Sage Erickson']],
                        {:multiple => true} %>
    <%= f.submit %>

<% end %>